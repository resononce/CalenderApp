@using SoftEng.DataAccess.DataObjects;
@{
    ViewData["Title"] = "User Home";
}
@model SoftEng.Models.UserHomeModel

<div align="center">
    <h2>User Home</h2>
    <ul>
        @foreach(Event e in this.Model.UserHomeEvents)
        {
            <li class="eventLeft"><span>@(e.Name)</span></li>
            <ul>
                <li class="eventLeft">@(e.EventDate.Date)</li>
                <li class="eventLeft">@(e.EventTime)</li>
            </ul>
        }
    </ul>
</div>